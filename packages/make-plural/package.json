    "vitest": "3.1.3",
    "vitest-sonar-reporter": "2.0.1"
  },
  "scripts": {
    "build": "tsc --noEmit && rslib build",
    "watch": "rslib watch",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "format": "biome format src",
    "check": "biome check src",
    "publish_to_npm": "npm publish"
