{
  "name": "@onigoetz/intl-formatters",
  "version": "0.1.0",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
